{
		"outline.showArrays": false,
		"outline.showBooleans": false,
		"outline.showConstants": false,
		"outline.showNull": false,
		"outline.showNumbers": false,
		"outline.showStrings": false,
		"outline.showObjects": false,
		"outline.showProperties": false,
		"outline.showTypeParameters": false,
		"outline.showVariables": false,
		"zenMode.hideTabs": false,
		"files.exclude": {
			"**/.git": true,
			"**/*.js": {
				"when": "$(basename).ts"
			},
			".vscode": true,
			"api": true,
			"backups": true,
			"bin": true,
			"content": true,
			"dist": true,
			"docs": true,
			"LICENSE.md": true,
			"node_modules": true,
			"notes": true,
			"package-lock.json": true,
			"project.komodoproject": true,
			"sass": true,
			"templates": true,
			"tools": true,
		},
		"files.watcherExclude": {
			"**/.git/objects/**": true,
			"**/.git/subtree-cache/**": true,
			"**/api/*/**": true,
			"**/backups/*/**": true,
			"**/node_modules/*/**": true
		},
		"git.enabled": false,
}
